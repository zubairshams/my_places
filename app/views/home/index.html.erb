<div class="page-header">
  <h1>Users</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Email</th>
      <th>Profile</th>
    </tr>
  </thead>
  <tbody>
    <% User.all.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= link_to 'Show', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>
